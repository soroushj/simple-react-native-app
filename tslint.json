{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-ordering": false,
    "max-line-length": false,
    "import-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}
